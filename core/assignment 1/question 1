public class Ques1{

	public static void main(String[] args) {
		int a=1, b=1, range, c, sum=0;
		float average=0;
		range=1;
		   while( range<=20 )
		   {
		     System.out.print(a +" ");
		      sum =sum+ a;
		      c = a + b;
		      a = b;
		      b = c;
		      range++;
		   }
		   average=sum/20;
		System.out.println("average is "+average);
	}

}


